{
  "name": "semantic-release-example",
  "version": "0.0.0-development",
  "description": "Example implementation of the semantic-release package",
  "main": "build/index.js",
  "scripts": {
    "codecov": "npx codecov",
    "commitmsg": "npx commitlint -e $GIT_PARAMS",
    "build": "npx babel src/ -d build/ --delete-dir-on-start",
    "build:prod": "BABEL_ENV=production npm run build",
    "lint": "npx eslint --ext .js .",
    "test": "npx jest",
    "prepublishOnly": "npm run build:prod",
    "gc": "commit",
    "semantic-commit": "semantic-commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "npx travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shageman/semantic-release-example.git"
  },
  "keywords": [
    "semantic-release"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shageman/semantic-release-example/issues"
  },
  "homepage": "https://github.com/shageman/semantic-release-example#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@commitlint/prompt-cli": "^8.3.5",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^25.1.0",
    "babel-preset-minify": "^0.5.1",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.2",
    "jest": "^25.1.0",
    "semantic-commit": "^0.2.1",
    "semantic-release": "^17.4.4",
    "travis-deploy-once": "^5.0.11",
    "utf-8-validate": "^5.0.2"
  },
  "dependencies": {},
  "release": {
    "branches": [
      "master", "origin\/master"
    ],
    "plugins":  [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "**/*",
              "label": "All files"
            }
          ]
        }
      ]
    ]
  }
}
